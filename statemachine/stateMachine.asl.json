{
    "Comment": "An example of the Amazon States Language using wait states",
    "StartAt": "SetInitialStatus",
    "States": {
        "SetInitialStatus": {
            "Type": "Task",
            "Resource": "${StepLambdaFunctionArn}",
            "Parameters": {
                "Payload.$": "$"
            },
            "Retry": [
                {
                    "ErrorEquals": [
                        "States.TaskFailed"
                    ],
                    "IntervalSeconds": 20,
                    "MaxAttempts": 5,
                    "BackoffRate": 10
                }
            ],
            "OutputPath": "$",
            "End": true
        }
    }
}